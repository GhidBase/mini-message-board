<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Sup</title>
    </head>
    <body>
        <h1>Message Board</h1>
        <%- message %>
        <ul>
            <% messages.forEach((message) => { %>
                <%- include('message',{
                    text: message.text,
                    user: message.user,
                    added: message.added,
                    id: message.id,
                }) %>
            <% }); %>
        </ul>
        <a href="new">New message</a>
    </body>
</html>
